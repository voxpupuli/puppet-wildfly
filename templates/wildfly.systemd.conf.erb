# The configuration you want to run
WILDFLY_CONFIG="'<%= scope.lookupvar('wildfly::config') %> -P <%= scope.lookupvar('wildfly::dirname') %>/jboss.properties'"

# The mode you want to run
WILDFLY_MODE=<%= scope.lookupvar('wildfly::mode') %>

# The address to bind to
WILDFLY_BIND=<%= scope.lookupvar('wildfly::properties')['jboss.bind.address'] %>

# Location of JDK
JAVA_HOME=<%= scope.lookupvar('wildfly::java_home') %>
